{
  "compilerOptions": {
    "jsx": "react-jsx",
    "incremental": true,
    "target": "es2021",
    "module": "commonjs",
    "lib": ["dom", "es2021"],
    "declaration": true,
    "declarationMap": true,
    "strict": false,
    "pretty": true,
    "sourceMap": true,
    "baseUrl": "./src",
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": "dist"
  },
  "include": ["src","type.d.ts"],
  "exclude": [".history", "node_modules"]
}
